{
  "description": "Send meaningless messages to #general.",
  "build": "faaslang",
  "timeout": 20000,
  "events": [
    {
      "scheduler.hourly": {
        "filename": "events/scheduler/hourly/ping.js",
        "subtype": {
          "offset": "0 minutes"
        }
      }
    },
    {
      "slack.command": {
        "filename": "events/slack/command/echo.js",
        "subtype": {
          "command": "echo"
        }
      }
    }
  ]
}
