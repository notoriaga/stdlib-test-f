{
  "description": "Send meaningless messages to #general.",
  "build": "faaslang",
  "timeout": 20000,
  "events": [
    {
      "slack.message_action": {
        "filename": "events/scheduler/hourly/ping.js",
        "subtype": {
          "callback_id": "acallbackid"
        }
      }
    },
    {
      "slack.command": {
        "filename": "events/slack/command/echo.js",
        "subtype": {
          "command": "echo"
        }
      }
    },
    {
      "slack.command": {
        "filename": "events/slack/command/another.js",
        "subtype": {
          "command": "another"
        }
      }
    }
  ]
}
